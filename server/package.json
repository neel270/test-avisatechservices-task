{
  "name": "courier-service-backend",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon ./src/index.ts",
    "start": "node ./src/index.ts",
    "build": "tsc",
    "lint": "eslint src/",
    "db:migrate": "sequelize db:migrate --require ts-node/register/transpile-only --config ./src/sequelize.config.js --migrations-path ./src/src/migrations",
    "db:migrate:undo": "sequelize db:migrate:undo --require ts-node/register/transpile-only --config ./src/sequelize.config.js --migrations-path ./src/src/migrations",
    "db:migrate:undo:all": "sequelize db:migrate:undo:all --require ts-node/register/transpile-only --config ./src/sequelize.config.js --migrations-path ./server/src/migrations"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.3.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "mysql2": "^3.11.3",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.16.5",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
